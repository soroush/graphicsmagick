#include "../Magick.tmpl"

XTPObjects= xtp.o network.o regular.o

PROGRAMS= xtp

AllTarget($(PROGRAMS))

NormalProgramTarget(xtp,$(XTPObjects),NullParameter,NullParameter,NullParameter)
InstallProgram(xtp,$(LOCALDIR))
InstallMultipleMan(xtp.1,$(LOCALMAN1))
DependTarget()
