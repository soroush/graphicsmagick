# Figure out what drivers Ghostscript supports (if available)
if test $have_gs = 'yes'
then

  # GSColorDevice
  echo $ac_n "checking for gs color device""... $ac_c" 1>&6
echo "configure:8511: checking for gs color device" >&5
  if $PSDelegate -q -dBATCH -sDEVICE=pnmraw -sOutputFile=/dev/null < /dev/null 2> /dev/null
  then
    GSColorDevice=pnmraw
  else
    if $PSDelegate -q -dBATCH -sDEVICE=ppmraw -sOutputFile=/dev/null < /dev/null 2> /dev/null
    then
      GSColorDevice=ppmraw
    else
      GSColorDevice=ppmraw
    fi
  fi
  
  echo "$ac_t""$GSColorDevice" 1>&6

  # GSMonoDevice
  echo $ac_n "checking for gs mono device""... $ac_c" 1>&6
echo "configure:8528: checking for gs mono device" >&5
  if $PSDelegate -q -dBATCH -sDEVICE=pbmraw -sOutputFile=/dev/null < /dev/null 2> /dev/null
  then
    GSMonoDevice=pbmraw
  else
    GSMonoDevice=ppmraw
  fi
  
  echo "$ac_t""$GSMonoDevice" 1>&6

  # GSPDFDevice
  echo $ac_n "checking for gs PDF writing device""... $ac_c" 1>&6
echo "configure:8540: checking for gs PDF writing device" >&5
  if $PSDelegate -q -dBATCH -sDEVICE=pdfwrite -sOutputFile=/dev/null < /dev/null 2> /dev/null
  then
    GSPDFDevice=pdfwrite
  else
    GSPDFDevice=nodevice
  fi
  
  echo "$ac_t""$GSPDFDevice" 1>&6

  # GSPSDevice
  echo $ac_n "checking for gs PS writing device""... $ac_c" 1>&6
echo "configure:8552: checking for gs PS writing device" >&5
  if $PSDelegate -q -dBATCH -sDEVICE=pswrite -sOutputFile=/dev/null < /dev/null 2> /dev/null
  then
    GSPSDevice=pswrite
  else
    GSPSDevice=nodevice
  fi
  
  echo "$ac_t""$GSPSDevice" 1>&6

  # GSEPSDevice
  echo $ac_n "checking for gs EPS writing device""... $ac_c" 1>&6
echo "configure:8564: checking for gs EPS writing device" >&5
  if $PSDelegate -q -dBATCH -sDEVICE=epswrite -sOutputFile=/dev/null < /dev/null 2> /dev/null
  then
    GSEPSDevice=epswrite
  else
    GSEPSDevice=nodevice
  fi
  
  echo "$ac_t""$GSEPSDevice" 1>&6

fi

