  # src tree.
  ac_file_inputs=`IFS=:
    for f in $ac_file_in; do
      case $f in
      -) echo $tmp/stdin ;;
      [\\/$]* | ?:[\\/]*)
         # Absolute
         test -f "$f" || {  echo "$as_me: error: cannot find input file: $f" >&2
  { (exit 1); exit; }; }
         echo $f;;
      *) # Relative
