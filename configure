  case $ac_old_set,$ac_new_set in
    set,)
      { echo "$as_me:877: WARNING: \`$ac_var' was set to \`$ac_old_val' in the previous run" >&5
echo "$as_me: warning: \`$ac_var' was set to \`$ac_old_val' in the previous run" >&2; }
      ac_suggest_removing_cache=: ;;
    ,set)
      { echo "$as_me:881: WARNING: \`$ac_var' was not set in the previous run" >&5
echo "$as_me: warning: \`$ac_var' was not set in the previous run" >&2; }
      ac_suggest_removing_cache=: ;;
    ,);;
    *)
      if test "x$ac_old_val" != "x$ac_new_val"; then
        { echo "$as_me:887: WARNING: \`$ac_var' has changed since the previous run:" >&5
echo "$as_me: warning: \`$ac_var' has changed since the previous run:" >&2; }
        { echo "$as_me:889: WARNING:   former value:  $ac_old_val" >&5
echo "$as_me: warning:   former value:  $ac_old_val" >&2; }
        { echo "$as_me:891: WARNING:   current value: $ac_new_val" >&5
echo "$as_me: warning:   current value: $ac_new_val" >&2; }
        ac_suggest_removing_cache=:
      fi;;
  esac
done
