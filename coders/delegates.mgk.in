<?xml version="1.0"?>
<delegatemap>
  <delegate decode="browse" stealth="True" spawn="True" command='@BrowseDelegate@ http://www.imagemagick.org/ImageMagick/'  />
  <delegate decode="bzip" stealth="True" command='|@BZIPDelegate@ -cd "%i"' />
  <delegate decode="cgm" command='@CGMDecodeDelegate@ -d ps "%i" "%o" %u
    @ConvertDelegate@ -concatenate "%o"*.ps "%o"' />
  <delegate decode="compress" stealth="True" command='|@LZWDecodeDelegate@ -c "%i"' />
  <delegate decode="dot" command='@DOTDecodeDelegate@ -Tps "%i" -o "%o"' />
  <delegate decode="dvi" command='@DVIDecodeDelegate@ -q -o "%o" "%i"' />
  <delegate decode="edit" stealth="True" command='@EditorDelegate@ -title "Edit Image Comment" -e vi "%o"' />
  <delegate decode="emf" command='@WMFDecodeDelegate@ "%i" "%o"' />
  <delegate decode="eps" encode="pdf" mode="bi" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSPDFDevice@ -sOutputFile=%o -- "%i" -c quit' />
  <delegate decode="eps" encode="ps" mode="bi" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSPSDevice@ -sOutputFile=%o -- "%i" -c quit' />
  <delegate decode="fig" command='@FIGDecodeDelegate@ -L ps "%i" "%o"' />
  <delegate decode="gplt" command='@EchoDelegate@ "set size 1.25,0.62
    set terminal postscript portrait color solid; set output "%o"; load "%i"" > %u;@GnuplotDecodeDelegate@ %u' />
  <delegate decode="gs-color" stealth="True" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSColorDevice@ -dTextAlphaBits=%u -dGraphicsAlphaBits=%u -g%s -r%s %s -sOutputFile=%s -- "%s" -c quit' />
  <delegate decode="gs-mono" stealth="True" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSMonoDevice@ -dTextAlphaBits=%u -dGraphicsAlphaBits=%u -g%s -r%s %s -sOutputFile=%s -- "%s" -c quit' />
  <delegate decode="hpgl" command='@HPGLDecodeDelegate@ -q -m eps -f `basename "%o"` "%i"
    mv `basename "%o"` "%o' />
  <delegate decode="htm" command='@HTMLDecodeDelegate@ -U -o "%o" "%i"' />
  <delegate decode="html" command='@HTMLDecodeDelegate@ -U -o "%o" "%i"' />
  <delegate decode="ilbm" command='@ILBMDecodeDelegate@ "%i" > "%o"' />
  <delegate decode="m2v" command='@MPEGDecodeDelegate@ -q -b "%i" -f -r -o3 "%o%%05d"
    @ConvertDelegate@ -concatenate "%o*.ppm" "%o"' />
  <delegate decode="man" command='@MANDelegate@ -man -Tps "%i" > "%o"' />
  <delegate decode="mpeg" command='@MPEGDecodeDelegate@ -q -b "%i" -f -r -o3 "%o%%05d"
    @ConvertDelegate@ -concatenate "%o*.ppm" "%o"' />
  <delegate decode="mpg" command='@MPEGDecodeDelegate@ -q -b "%i" -f -r -o3 "%o%%05d"
    @ConvertDelegate@ -concatenate "%o*.ppm" "%o"' />
  <delegate decode="pdf" encode="eps" mode="bi" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSEPSDevice@ -sOutputFile=%o -- "%i" -c quit' />
  <delegate decode="pdf" encode="ps" mode="bi" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSPSDevice@ -sOutputFile=%o -- "%i" -c quit' />
  <delegate decode="pgp" command='@PGPDecodeDelegate@ -fq "%i"' />
  <delegate decode="pnm" encode="ilbm" mode="encode" command='@ILBMEncodeDelegate@ -24if "%i" > "%o"' />
  <delegate decode="pnm" encode="launch" mode="encode" command='@LaunchDelegate@ "%i"' />
  <delegate decode="pnm" encode="win" mode="encode" command='@ShowImageDelegate@ -immutable "%i"' />
  <delegate decode="pov" command='@POVDelegate@ "+i"%i"" +o"%o" +fn%q +w%w +h%h +a -q9 -kfi"%s" -kff"%n"
    @ConvertDelegate@ -concatenate "%o*.png" "%o"' />
  <delegate decode="ps" encode="eps" mode="bi" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSEPSDevice@ -sOutputFile=%o -- "%i" -c quit' />
  <delegate decode="ps" encode="pdf" mode="bi" command='@PSDelegate@ -q -dBATCH -dMaxBitmap=300000000 -dNOPAUSE -dSAFER -sDEVICE=@GSPDFDevice@ -sOutputFile=%o -- "%i" -c quit' />
  <delegate decode="ps" encode="print" mode="encode" command='@PrintDelegate@ "%i"' />
  <delegate decode="rad" command='@RADDecodeDelegate@ -g 1.0 "%i" "%o"' />
  <delegate decode="rgba" encode="rle" mode="encode" command='@MogrifyDelegate@ -flip -size %wx%h "rgba:%i"
    @RLEEncodeDelegate@ -w %w -h %h -n 4 -o "%o" "%i"' />
  <delegate decode="scan" command='@SCANDecodeDelegate@ -d "%i" > "%o"' />
  <delegate decode="shtml" command='@HTMLDecodeDelegate@ -U -o "%o" "%i"' />
  <delegate decode="txt" encode="ps" mode="bi" command='@TXTDelegate@ -o "%o" "%i"' />
  <delegate decode="wmf" command='@WMFDecodeDelegate@ "%i" "%o"' />
  <delegate decode="xcf" command='@XCFDecodeDelegate@ "%i" > "%o"' />
  <delegate decode="yuv" encode="m2v" mode="encode" command='@MPEGEncodeDelegate@ %u "%o"
    @ConvertDelegate@ -concatenate "%i*.yuv" %u' />
  <delegate decode="yuv" encode="mpeg" mode="encode" command='@MPEGEncodeDelegate@ %u "%o"
    @ConvertDelegate@ -concatenate "%i*.yuv" %u' />
  <delegate decode="yuv" encode="mpg" mode="encode" command='@MPEGEncodeDelegate@ %u "%o"
    @ConvertDelegate@ -concatenate "%i*.yuv" %u' />
  <delegate decode="zip" stealth="True" command='|@ZipDelegate@ -cdfq "%i"' />
  <delegate encode="bzip" stealth="True" command='|@BZIPDelegate@ -f > "%o"' />
  <delegate encode="compress" stealth="True" command='|@LZWEncodeDelegate@ -c > "%o"' />
  <delegate encode="show" stealth="True" spawn="True" command='@ShowImageDelegate@ -immutable -window_group %g -title "%l of %f" "tmp:%o"' />
  <delegate encode="zip" stealth="True" command='|@ZipDelegate@ -cf > "%o"' />
</delegatemap>
